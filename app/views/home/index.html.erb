<h1>Home</h1>

<ul>
  <% @users.each do |user| %>
    <li><%= user.username %>
       <%= link_to image_tag("star.png"), {:upvote => user.id} %>
       <span class="vote-number"><%= user.get_likes.size %></span>
       <%= link_to "Text now!", user_path(user)  %>
    </li>

  <% end %>
</ul>

